<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>b781df52-32b0-4660-8449-3351296068fa</Id>
  <Version>1.1.0.0</Version>
  <ProviderName>DataVista</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="DataVista Premium Export"/>
  <Description DefaultValue="Offline interactive HTML reports for Excel data."/>
  <IconUrl DefaultValue="https://Harry-0402.github.io/DataVista/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://Harry-0402.github.io/DataVista/assets/icon-80.png"/>
  <SupportUrl DefaultValue="https://www.contoso.com/help"/>
  <AppDomains>
    <AppDomain>https://Harry-0402.github.io</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://Harry-0402.github.io/DataVista/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <CustomTab id="TabOfficeAddinTest">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="BtnExportSelection">
                  <Label resid="BtnExportSelection.Label"/>
                  <Supertip>
                    <Title resid="BtnExportSelection.Label"/>
                    <Description resid="BtnExportSelection.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>exportSelection</FunctionName>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="BtnExportWorkbook">
                  <Label resid="BtnExportWorkbook.Label"/>
                  <Supertip>
                    <Title resid="BtnExportWorkbook.Label"/>
                    <Description resid="BtnExportWorkbook.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>exportWorkbook</FunctionName>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
              <Label resid="TabOfficeAddinTest.Label"/>
            </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://Harry-0402.github.io/DataVista/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://Harry-0402.github.io/DataVista/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://Harry-0402.github.io/DataVista/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://Harry-0402.github.io/DataVista/"/>
        <bt:Url id="Commands.Url" DefaultValue="https://Harry-0402.github.io/DataVista/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://Harry-0402.github.io/DataVista/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with DataVista!"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Export Tools"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="About / Support"/>
        <bt:String id="BtnExportSelection.Label" DefaultValue="Export Selection"/>
        <bt:String id="BtnExportWorkbook.Label" DefaultValue="Export Workbook"/>
        <bt:String id="TabOfficeAddinTest.Label" DefaultValue="DataVista"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Your new DataVista add-in loaded successfully. Go to the DataVista tab to start using it!"/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Open the help guide and support information."/>
        <bt:String id="BtnExportSelection.Tooltip" DefaultValue="Generate report for the currently selected cells."/>
        <bt:String id="BtnExportWorkbook.Tooltip" DefaultValue="Generate a comprehensive report for the entire workbook."/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
